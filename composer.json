{
    "name": "adeoweb/custom-entity-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Custom entity bundle",
    "keywords": ["pim", "akeneo", "ecommerce", "integration", "reference data"],
    "homepage": "http://www.akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0",
        "akeneo/pim-community-dev": "^7.0.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.14.0"
    },
    "replace": {
        "akeneo-labs/custom-entity-bundle": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Pim\\Bundle\\CustomEntityBundle\\": "",
            "Acme\\Bundle\\CustomBundle\\": "docs/examples/CustomBundle"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
